<% include components/_head %>
<div class="col-md-12 Home animated fadeIn text-center">
	<div class="col-md-12 home-top">
		<div class="col-md-12 home-slide">
			<div class="col-md-12 Slide carousel slide" data-ride="carousel">
				<div class="carousel-inner">
					<div class="item active animated flipInY">
						<div class="carousel-caption">
							<% include components/_description-company %>
						</div>
					</div>
					<div class="item animated flipInY">
						<div class="carousel-caption">
							<% include components/_facts-box %>
						</div>
					</div>
					<div class="item animated flipInY">
						<div class="carousel-caption">
							<% include components/_advantages %>
						</div>
					</div>
				</div>
				<a class="left carousel-control" href=".Slide" role="button" data-slide="prev">
					<span aria-hidden="true"><img src="./images/left.svg" /></span>
				</a>
				<a class="right carousel-control" href=".Slide" role="button" data-slide="next">
					<span aria-hidden="true"><img src="./images/right.svg" /></span>
				</a>
			</div>
		</div>
	</div>
</div>
